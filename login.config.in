com.sun.security.jgss.initiate {
    com.sun.security.auth.module.Krb5LoginModule required
       client=true
       storeKey=true
       useKeyTab=true
       keyTab="/etc/krb5.keytab-cxf-svc"
       principal="cxf-svc-test@SU.SE"
       doNotPrompt=true
       realm="SU.SE"
       debug="false"
    ;
};

